{
  "name": "gulp-webdriver",
  "version": "2.0.3",
  "description": "gulp-webdriver is a gulp plugin to run selenium tests with the WebdriverIO testrunner",
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio/gulp-webdriver.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "babel --presets es2015 -d lib/ src/",
    "eslint": "eslint gulp/ src/ test/ *.js",
    "test": "gulp test",
    "pretest": "npm run eslint && npm run compile"
  },
  "keywords": [
    "webdriverio",
    "gulp",
    "selenium",
    "webdriver",
    "mocha",
    "test",
    "testing",
    "bdd",
    "tdd",
    "saucelabs",
    "runner",
    "gulpplugin"
  ],
  "author": "Christian Bromann <mail@christian-bromann.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/gulp-webdriver/issues"
  },
  "homepage": "https://github.com/webdriverio/gulp-webdriver",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "gulp-util": "^3.0.8",
    "resolve": "^1.4.0",
    "through2": "^2.0.3"
  },
  "peerDependencies": {
    "webdriverio": "^4.1.1",
    "@wdio/cli": "^5.11.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "codeclimate-test-reporter": "^0.5.0",
    "del": "^3.0.0",
    "eslint": "^4.4.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "4.0.0",
    "mocha": "^3.0.8",
    "run-sequence": "^2.1.0",
    "selenium-standalone": "^6.7.0",
    "wdio-mocha-framework": "^0.5.11"
  }
}
