{
  "name": "gulp-webdriver",
  "version": "2.0.1",
  "description": "gulp-webdriver is a gulp plugin to run selenium tests with the WebdriverIO testrunner",
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio/gulp-webdriver.git"
  },
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --presets es2015 -d lib/ src/",
    "eslint": "eslint gulp/ src/ test/ *.js",
    "test": "gulp test",
    "pretest": "npm run eslint && npm run compile"
  },
  "keywords": [
    "webdriverio",
    "gulp",
    "selenium",
    "webdriver",
    "mocha",
    "test",
    "testing",
    "bdd",
    "tdd",
    "saucelabs",
    "runner",
    "gulpplugin"
  ],
  "author": "Christian Bromann <mail@christian-bromann.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/gulp-webdriver/issues"
  },
  "homepage": "https://github.com/webdriverio/gulp-webdriver",
  "dependencies": {
    "babel-polyfill": "^6.7.2",
    "gulp-util": "^3.0.7",
    "resolve": "^1.1.7",
    "through2": "^0.6.5"
  },
  "peerDependencies": {
    "webdriverio": "^4.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "codeclimate-test-reporter": "^0.3.1",
    "del": "^2.2.0",
    "eslint": "^1.4.1",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.0",
    "gulp": "^3.8.11",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "1.0.0",
    "gulp-util": "^3.0.7",
    "mocha": "^2.3.0",
    "run-sequence": "^1.1.5",
    "selenium-standalone": "^5.0.0",
    "wdio-mocha-framework": "^0.2.12"
  }
}
